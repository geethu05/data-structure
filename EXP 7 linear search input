#include<stdio.h>
int main()
{
	int i,n,A[20],key,flag=0;
	printf("enter the element:");
	scanf("%d",&n);
	printf("enter an array of elements:");
	for(i=0;i<n;i++)
	scanf("%d",&A[i]);
	printf("enter the element to be search");
	scanf("%d",&key);
	
	for(i=0;i<n;i++)
	{
		(A[i]==key);
		{
			flag=1;
			break;
		}
	}
	if (flag==1)
    	printf("the element is foumd");
	else
	    printf("the element is not found");
}
	 
