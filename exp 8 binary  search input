#include<stdio.h>
int main()
{
	 int i,n,A[50],key,flag=0,first,last,mid;
	 printf("enter number of element:");
     scanf("%d",&n);
	 printf("enter the  n array of the element ");
	 for(i=0;i<n;i++)	
	 {
	 	scanf("%d",&A[i]);

	  } 
	  printf("enter the element to search");
	  scanf("%d",&key);
	  first=0;
	  last=n-1;
	  mid=(first+last)/2;
	  while(first<=last){
	  	mid=(first+last)/2;
	  	if(A[mid]==key)
	  	
		{
		    flag=1;	
			break;
			
    	  }
    	  else if(A[mid]<key)
    	  first=mid+1;
    	  else if (A[mid]>key)
    	  last=mid-1;
    	  
	  }
	  if (flag==1)
	  printf("the key element is found");
	  else
	  printf("key element is not found");
	  
	 }
